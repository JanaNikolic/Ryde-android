<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.App_Tim17" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/teal</item>
        <item name="colorPrimaryVariant">@color/dark_teal</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal</item>
        <item name="colorSecondaryVariant">@color/teal</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:navigationBarColor">@color/teal</item>
        <!-- Customize your theme here. -->
        <item name="android:textColorHint">@color/teal</item>
        <item name="colorControlNormal">#c5c5c5</item>
        <item name="colorControlActivated">@color/teal</item>
        <item name="colorControlHighlight">@color/teal</item>
    </style>
    <style name="NavigationBar">
        <item name="colorPrimary">@color/yellow</item>
        <item name="android:textColorSecondary">@color/white</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorSurface">@color/teal</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="ThemeOverlay.App_Tim17.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>
</resources>